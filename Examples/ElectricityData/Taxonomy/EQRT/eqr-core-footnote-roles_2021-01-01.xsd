<?xml version='1.0' encoding='UTF-8'?>
<!-- Schema for FERC footnote types -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:link="http://www.xbrl.org/2003/linkbase" xmlns:xbrli="http://www.xbrl.org/2003/instance" xmlns:xlink="http://www.w3.org/1999/xlink" elementFormDefault="qualified" targetNamespace="http://www.ferc.gov/eqr/roles/footnote">
   <xs:import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
   <xs:annotation>
      <xs:appinfo>
         <link:arcroleType arcroleURI="http://www.ferc.gov/arcrole/Confidential" cyclesAllowed="any" id="confidential">
